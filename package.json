{
  "name": "power-play-tech",
  "version": "1.0.0",
  "description": "eCommerce app built with the MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js -i",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SherifAckles/power-play-tech-ecommerce.git"
  },
  "author": "Sherif Moustafa AKA Ackles",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/SherifAckles/power-play-tech-ecommerce/issues"
  },
  "homepage": "https://github.com/SherifAckles/power-play-tech-ecommerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "express": "^4.18.2",
    "mongoose": "^7.3.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "nodemon": "^2.0.22"
  }
}
